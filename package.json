{
  "name": "@karlmarxlopez/expo-ts-and-jest",
  "description": "Expo with TypeScript and Jest",
  "version": "0.0.3",
  "main": "node_modules/expo/AppEntry.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "expo start",
    "start:devclient": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "reset-watchman": "watchman watch-del .. ; watchman watch-project ..",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "@react-native-community/netinfo": "9.3.0",
    "@react-navigation/native": "6.0.11",
    "@react-navigation/stack": "6.2.2",
    "@reduxjs/toolkit": "1.8.3",
    "@rneui/base": "4.0.0-rc.5",
    "@rneui/themed": "4.0.0-rc.5",
    "expo": "45.0.6",
    "expo-community-flipper": "45.1.0",
    "expo-dev-client": "1.0.0",
    "expo-status-bar": "1.3.0",
    "jest": "26.6.3",
    "jest-expo": "45.0.1",
    "react": "17.0.2",
    "react-devtools-core": "4.24.7",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-flipper": "^0.151.1",
    "react-native-gesture-handler": "2.5.0",
    "react-native-reanimated": "2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "3.14.1",
    "react-native-size-matters": "0.4.0",
    "react-native-web": "0.17.7",
    "react-native-webview": "11.22.5",
    "react-redux": "8.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@react-native-community/eslint-config": "3.0.3",
    "@testing-library/jest-native": "4.0.5",
    "@testing-library/react-native": "10.1.1",
    "@types/jest": "28.1.3",
    "@types/react-native": "~0.66.13",
    "@types/react-test-renderer": "17",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "eslint": "7",
    "eslint-import-resolver-typescript": "3.1.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-react-native-a11y": "3.2.1",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "2.7.1",
    "react-test-renderer": "17",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "resolutions": {
    "@types/react": "~17.0.21"
  }
}
