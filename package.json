{
  "name": "@karlmarxlopez/expo-ts-and-jest",
  "description": "Expo with TypeScript and Jest",
  "version": "0.1.2",
  "license": "MIT",
  "main": "node_modules/expo/AppEntry.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "expo start",
    "start:devclient": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "prepare": "husky install",
    "reset-watchman": "watchman watch-del . ; watchman watch-project .",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand"
  },
  "dependencies": {
    "@react-native-community/netinfo": "9.3.2",
    "@react-navigation/native": "6.0.13",
    "@react-navigation/stack": "6.3.1",
    "@reduxjs/toolkit": "1.8.5",
    "@rneui/base": "4.0.0-rc.6",
    "@rneui/themed": "4.0.0-rc.6",
    "expo": "^46.0.13",
    "expo-community-flipper": "46.0.1",
    "expo-dev-client": "~1.2.1",
    "expo-status-bar": "~1.4.0",
    "react": "18.0.0",
    "react-devtools-core": "4.26.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-flipper": "^0.164.0",
    "react-native-gesture-handler": "~2.5.0",
    "react-native-reanimated": "~2.9.1",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.17.0",
    "react-native-size-matters": "0.4.0",
    "react-native-web": "~0.18.7",
    "react-native-webview": "11.23.0",
    "react-redux": "8.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@react-native-community/eslint-config": "3.1.0",
    "@testing-library/jest-native": "4.0.13",
    "@testing-library/react-native": "11.2.0",
    "@types/jest": "28.1.3",
    "@types/react-native": "~0.69.1",
    "@types/react-test-renderer": "18",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "cspell": "6.10.1",
    "eslint": "7",
    "eslint-import-resolver-typescript": "3.5.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-react-native-a11y": "3.2.1",
    "husky": ">=6",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.0",
    "lint-staged": ">=10",
    "prettier": "2.7.1",
    "react-test-renderer": "18.2.0",
    "typescript": "^4.8.3"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,js,css,md}": "prettier --write",
    "*": "cspell lint --no-summary *"
  },
  "resolutions": {
    "@types/react": "~17.0.21"
  }
}
